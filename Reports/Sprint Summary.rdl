    <DataSet Name="Burndown">
      <Query>
        <DataSourceName>TfsCompanionDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SprintID">
            <Value>=Parameters!IterationID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TeamID">
            <Value>=Parameters!TeamID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @SprintID INT = 3
--DECLARE @TeamID   INT = 707

DECLARE @DateStart DATETIME
DECLARE @DateEnd   DATETIME
DECLARE @NowDate   DATETIME
DECLARE @MaxWork   FLOAT
DECLARE @Days      INT

SELECT @NowDate = MAX(LastUpdateDate)
FROM dbo.ExtractLog
WHERE Latest=1

SELECT
  @DateStart = CAST(DateStart AS DATE)
 ,@DateEnd   = CAST(DateEnd   AS DATE)
FROM dbo.Iteration 
WHERE ID = @SprintID

-- get relevant work items for team areas
IF OBJECT_ID('tempdb..#tmpWork','U') IS NOT NULL
DROP TABLE #tmpWork

CREATE TABLE #tmpWork(
  ID       INT
 ,Rev      INT
 ,DateFrom DATETIME
 ,DateTo   DATETIME
 ,RemainingWork FLOAT
)

INSERT INTO #tmpWork(
  ID
 ,Rev
 ,DateFrom
 ,DateTo
 ,RemainingWork
)
SELECT
  w.ID
 ,w.Rev
 ,w.DateFrom
 ,w.DateTo
 ,w.RemainingWork

FROM dbo.WorkItem w
INNER JOIN dbo.TeamArea a
  ON a.AreaID = w.AreaID
 AND a.TeamID = @TeamID
WHERE w.WorkItemType = 'Task'
  AND w.IterationID = @SprintID

CREATE UNIQUE CLUSTERED INDEX cidx_Work
ON #tmpWork(ID,Rev)

--SELECT * FROM #tmpWork

-- construct chart data
IF OBJECT_ID('tempdb..#tmpChart','U') IS NOT NULL
DROP TABLE #tmpChart

CREATE TABLE #tmpChart(
  [DayNum]       INT IDENTITY(0,1)
 ,[Date]         DATETIME NOT NULL
 ,[BurnLine]     FLOAT
 ,[TrendLine]    FLOAT
)

INSERT INTO #tmpChart(
  [Date]
 ,[BurnLine]
)

-- Sprint burndown data
SELECT 
  c.[Date]
 ,CASE WHEN c.[Date]&gt;DATEADD(DAY,1,@NowDate) THEN NULL ELSE SUM(w.RemainingWork) END AS BurnLine

FROM dbo.Calendar c

-- get Sprint work item version that existed on calendar date
LEFT JOIN #tmpWork w
   ON c.[Date] BETWEEN w.[DateFrom] AND w.[DateTo]

 -- only get data for Sprint days
WHERE c.[Date] BETWEEN @DateStart AND @DateEnd

GROUP BY c.[Date]
ORDER BY c.[Date]

-- get Sprint info
SELECT
  @DateStart = MIN([Date])
 ,@DateEnd   = MAX([Date])
 ,@MaxWork   = MAX([BurnLine])
 ,@Days      = COUNT(DISTINCT [Date])-1
FROM #tmpChart

-- add trendline
UPDATE #tmpChart SET TrendLine = @MaxWork * (@Days-[DayNum])/@Days

SELECT 
  [Date]
 ,[BurnLine]
 ,[TrendLine]
FROM #tmpChart</CommandText>
      </Query>
