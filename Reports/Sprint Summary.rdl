    <DataSet Name="SprintBacklog">
      <Query>
        <DataSourceName>TfsCompanionDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TeamID">
            <Value>=Parameters!TeamID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SprintID">
            <Value>=Parameters!IterationID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  p.ID     AS PbiID
 ,p.Title  AS PbiTitle
 ,p.Effort AS PbiEffort
 ,p.State  AS PbiState
 ,p.RemainingWork AS PbiRemainingWork
 ,p.BacklogPriority AS PbiPriority
 ,up.DisplayName  AS PbiAssignedTo
 ,t.ID     AS TaskID
 ,t.Title  AS TaskTitle
 ,t.RemainingWork AS TaskRemainingWork
 ,t.State  AS TaskState
 ,t.BacklogPriority AS TaskPriority
 ,ut.DisplayName  AS TaskAssignedTo

FROM dbo.WorkItem p
INNER JOIN dbo.TeamArea a
  ON a.AreaID = p.AreaID
 AND a.TeamID = @TeamID

LEFT JOIN dbo.Users up
  ON up.UserID = p.AssignedToID

LEFT JOIN dbo.WorkItem t
  ON t.ParentID = p.ID
 AND t.WorkItemType = 'Task'
 AND t.Latest=1

LEFT JOIN dbo.Users ut
  ON ut.UserID = t.AssignedToID

WHERE p.WorkItemType = 'Product Backlog Item'
  AND p.Latest=1
  AND p.IterationID = @SprintID</CommandText>
      </Query>
